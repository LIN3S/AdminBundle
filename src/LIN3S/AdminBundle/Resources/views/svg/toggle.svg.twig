{% set rect_size = 10 %}
{% set viewbox_size = 50 %}

<svg viewBox="0 0 {{ viewbox_size }} {{ viewbox_size }}" class="toggle">
    <rect height="{{ rect_size }}" width="{{ viewbox_size }}" x="0" y="{{ viewbox_size / 2 - rect_size / 2}}"/>
    <rect height="{{ viewbox_size }}" width="{{ rect_size }}" x="{{ viewbox_size / 2 - rect_size / 2}}" y="0"/>
</svg>
